doctype 5
html
  head
    title= title
    meta(name="description", content="javascript a příbuzná témata")
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript', src='http://code.jquery.com/jquery-1.10.1.min.js')
    link(rel="icon",type="image/png",href="http://borakpetr/images/favicon.png")
    link(href="data:image/png;base64,", rel="icon", type="image/x-icon")
      
      
    script(type='text/javascript', src='/ckeditor/ckeditor.js')
    script(type='text/javascript') 
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      
        ga('create', 'UA-42618087-1', 'borakpetr.cz');
        ga('send', 'pageview');
       
    
  body(ng-app="main")                                              
    #container
      #contIn
        div#headerCzech
            a#backtoMainPage(alt='back to Petr Borák main page', href="http://www.borakpetr.cz/cz")
             h1 Petr Borák, webdevelopement, grafika, javascript.
            #ls
              #logIn
                - if(logged)
                    form(action="/session", method="POST")
                      input(type="hidden", name="_method", value="delete")
                      input#logOut(type="submit", value="Log out")
                - else
                    a(href="/session/new") Log In    
              
              #language
                a#englishHref(href=englishHref) English
                a#czechHref(href=czechHref) Czech
                                                
              #searchbox
                form(action="/search", method="get")
                  input#searchBox(type="text", name="search", value="")
                  input#submitSearch(type="submit", value="") 

                
            #mainMenu
              ul
                li
                  -if(actualSite == "index") 
                    a(href="/cz", class="actual") O mně
                  -else
                    a(href="/cz")   O mně
                li
                  -if(actualSite == "portfolio")  
                    a(href="/portfolio/webdesign/cz", class="actual") portfolio
                  -else
                    a(href="/portfolio/webdesign/cz") portfolio
                  
                li
                  -if(actualSite == "blog")  
                    a(href="/blog/cz", class="actual") blog    
                  -else
                    a(href="/blog/cz") blog    
                li
                  a(href="/technologie") technologie
                  
            #bottomMenu
              ul
                li(class='samsung')
                  a(href='/samsungStv/cz') samsungTv
                li(class='html')
                  a(href='/html5/cz') html5
                li(class='node')
                  a(href='/node/cz') node
                li(class='angular')
                  a(href='/angular/cz') angular
                
                  
          
        block content
        div.clearfix
      #footer
        #footerIn
          #contactMeFooter
              h3 Kontaktujte mě
              form(action="/contactForm", method="post", id="ctmainpage00")
                input#cm0(type="text", name="name", value="Vaše jméno")
                input#cm1(type="text", name="contact", value="kontakt na Vás (email or phone num.)")
                textarea#cm2(name="content") Poptávka nebo zpráva
                input(type="submit", value="Send")
              #noteUnderFooter Vše na těchto stránkách je vytvořeno vlastním úsilím mnou osobně. Pokud budete chtít cokoli využít, prosím. Neváhejte mne kontaktovat s návrhma nebo otázkama.
          #contactDetails
            h3 Kontaktní údaje
            h4 Stálé bydliště
            div(class="cdin") Petr Borák <br/> Zálesí 29<br/>790 70, Javorník ve Slezsku 
            h4 Přechodná adresa
            div.cdin Petr Borák <br/>Mírová 26 <br/>602 00, Brno  
            h4 Phone number
            div.cdin +420 604 478 311              
          
          div#thirdFooter

            a.links(href="skype:borak.petr3?call")            
              img#linkSkype(src="/images/skypeLink.png")
            script(type="text/javascript")
              $("#linkSkype").bind("click",function(){
                document.location = "skype:borak.petr3?call";
              });
            a.links(href="https://www.facebook.com/borak.petr?ref=tn_tnmn", target="_blank")
              img#linkSkype(src="/images/facebookLink.png")
            a.links(href="https://twitter.com/PetrBork", target="_blank")
              img#linkSkype(src="/images/twitterLink.png")              
            img(src="/images/borakFooter.png")
            p.copyright &copy; 2013 Petr Borák 

    script(type='text/javascript', src='/javascripts/angular.js')
    script(type='text/javascript', src='/javascripts/angular-resource.js')
    script(type='text/javascript', src='/javascripts/modules.js')
    script(type='text/javascript', src='/javascripts/header.js')
       